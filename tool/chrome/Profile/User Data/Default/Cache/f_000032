<script>

    $(function(){

        var oUlWidth=$("#js_link ul").width()*$("#js_link ul").length;
        var oLink=$("#js_link .link_warp");
        var Num=$("#js_link ul").length;
        var oLinkLeft,oLinkRight;
        oLink.width(oUlWidth*2);

        if($("#js_link ul").length<=3){
           $("#js_link .link_btn a").hide();

        }else{
            $("#js_link").mouseenter(function(){
              $("#js_link .link_btn a").fadeIn();
          });
          $("#js_link").mouseleave(function(){
              $("#js_link .link_btn a").fadeOut();
          });
        }

        $("#js_link .link_btn a").eq(0).click(function(){
            oLink.stop(true,true);
            if( parseInt( oLink.css("left") )<=-(Num-3)*140 ){

                oLink.css("left",-(Num-3)*140+"px");

            }else{
                oLink.animate({left: parseInt(oLink.css("left"))-140+'px'});
            }

        });

        $("#js_link .link_btn a").eq(1).click(function(){

            oLink.stop(true,true);
            if( parseInt( oLink.css("left") )>= 0 ){

                oLink.css("left","0");

            }else{
                oLink.animate({left: parseInt(oLink.css("left"))+140+'px'});
            }

        });
		link_change();
		$(window).resize(function(){
			link_change();  
		});	  
    })

	function link_change()
	{
		if($(window).width()>=1400)
		{
			$("#android_tit").html('IOS/Android下载');
			$("#iosdown").show();
			$("#footer .foot").width(1200);
			$("#footer .software").css("margin-left","30px");
			$("#anchorHelp").show();
		}
		else
		{
			$("#android_tit").html('IOS/Android下载');
			$("#iosdown").hide();
			$("#footer .foot").width(1000);
			$("#footer .software").css("margin-left","16px");
			$("#anchorHelp").hide();
		}
		
	}
</script>
