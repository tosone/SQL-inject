function show_near(){
    var timestamp =new Date().getTime();  //获取完整时间戳
    var now_time  = timestamp/1000;
    if(now_time >= $ROOM['near_show_time']){
        $('#js_play_remind').hide();
        clearTimeout(play_remind);
    }
}

