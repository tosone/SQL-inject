$(function(){      
    //自定义滚动条          
        var scrollWrapH = 442;    //滚动区域外围高度
        var scrollContH=0;   //滚动区域的内容高度
        var scrollTrackH = 442;  //滚动条轨道高度
        var wordWarp=0;
        var scrollThumbH = 0;     //滚动条按钮高度
        
        var speed=0;
             
       //滚动条高度
      

       $("#js_scroll_bar .s_bar span").mousedown(function(e){            
            var disY=e.pageY-parseInt($(this).css("top") ); 
           $(document).mousemove(function(e) {
               var t=e.pageY-disY; 
               var disBoxT=parseInt(  $("#js_scroll_bar .s_bar").height() - $("#js_scroll_bar .s_bar span").height());              
               if(t<0){
                 t=0
               }else if( t>= disBoxT ){
                  t=disBoxT;
               }
               $("#js_scroll_bar .s_bar span").css({
                    "top":t+"px"
                });                
                speed= -(t/disBoxT)*parseInt($("#js_scroll_bar .overview").height() - $("#js_scroll_bar").height());
                $("#js_scroll_bar .overview").css("top",speed+"px");
                return false;
           });
           $(document).mouseup(function(){
               $(document).unbind();
           });
           return false;
       }); 

    
    $("#js_scroll_bar .overview").mousewheel(function(e,d){
        if(!$("#js_scroll_bar .s_bar span").is(":hidden")){
                var w=parseInt($("#js_scroll_bar .overview").height() - $("#js_scroll_bar").height());
                if (d==1) {
                  speed+=12;
                }else if(d == -1){
                   speed-=12;
                }        
                if(speed <= -w ){
                  speed=-w;
                }        
                if(speed>=0){
                   speed=0;
                }

                $("#js_scroll_bar .overview").css("top",speed+"px");
                var tt= Math.abs(speed/w)*( parseInt(scrollTrackH - $("#js_scroll_bar .s_bar span").height()));      
                 $("#js_scroll_bar .s_bar span").css("top",tt+"px");
         }
         e.preventDefault();
    });

    var lastIndex = -1;  
    var b = false;
    $("#js_task_frame .task_navbar li").click(function(){
           var index = $(this).index();
           speed = parseInt($("#js_scroll_bar .overview").css("top"));
           if( !$(this).find('dl.task_list').is(":animated") ){           
               if(!b){                
                    $(this).addClass('active').siblings().removeClass('active');
                    $(this).find('dl.task_list').slideDown("fast",scrollHCount);
                    b=true;
               }else if(b && lastIndex==index){
                    $(this).removeClass('active');
                    $(this).find('dl.task_list').slideUp("fast",scrollCont_comm);
                    b=false;
               }else{
                     $("#js_task_frame .task_navbar li").eq(lastIndex).find('dl.task_list').slideUp("fast");
                     $(this).addClass('active').siblings().removeClass('active');
                     $(this).find('dl.task_list').slideDown("fast",scrollCont_comm);
                     b=true;
               }
           
         }
          lastIndex=index;
    });


    $("#js_task_frame  .task_des .task_des_box").find('.js_task_scrollbar').tinyscrollbar();
    //tab切换
    $("#js_task_frame .task_navbar li ").find('dd').click(function(event){
         var ind = $(this).index();
         $(this).addClass('cur').siblings().removeClass('cur');
         $('.js_task_name').html($(this).find("span")[0].outerHTML+'<em>'+$(this).data('task_name')+'</em>');
         $('.js_task_desc').html($(this).data('desc'));
         $('.js_total_gogle').html($(this).data('total_gold'));
         if($(this).hasClass('geted')){
             $('.js_task_btn').html('领取鱼丸');
             $('.js_task_btn').addClass('active');
             $('.js_task_btn').attr('jurl','');
             $('.js_task_btn').attr('rel',$(this).data('rel'));
         }else{
             $('.js_task_btn').html($(this).data('click_name'));
             $('.js_task_btn').attr('jurl',$(this).data('jurl'));
             $('.js_task_btn').removeClass('active');
             $('.js_task_btn').attr('rel','');
         }
         $('.js_task_btn').attr('tid',$(this).data('tid'));
         $('.js_task_btn').attr('aid',$(this).data('aid'));
         $("#js_task_frame  .task_des .task_des_box").find('.js_task_scrollbar').tinyscrollbar_update();
         event.stopPropagation();
    }); 
    

    function scrollHCount(){
         scrollContH=$("#js_scroll_bar .overview").height();
         wordWarp=scrollContH- $("#js_task_scroll").height();
         scrollThumbH = ($("#js_task_scroll").height()/scrollContH)*scrollTrackH;
        if(scrollContH>$("#js_scroll_bar").height()){
           $("#js_scroll_bar .s_bar span").css("display","block")
            $("#js_scroll_bar .s_bar span").animate({'height':scrollThumbH+'px'});
         }else{
            $("#js_scroll_bar .s_bar span").css("display","none")
         }
             
    }
    
    function scrollCont_comm(){
         scrollHCount();
        if(speed <= -wordWarp ){
          speed=-wordWarp;
        }
        if(speed>=0){
           speed=0;
        }
        $("#js_scroll_bar .overview").animate({"top":speed+"px"},300);        
        var tt= Math.abs(speed/wordWarp)*( parseInt(scrollTrackH - scrollThumbH));      
         $("#js_scroll_bar .s_bar span").animate({"top":tt+"px"},300);
    }

})
</script>

