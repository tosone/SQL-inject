function js_live_room(){
  if(typeof(is_room) !='undefined' && is_room == 1){
      room_live_room();
  }else{
      defaut_live_room();
  }
  $('#inter_tags').hide();
  $('#js_phone_div').hide();
}


$(function(){
  $('.r_close').click(function(){
      $('#js_phone_div').hide();
  });
});
</script><script type="text/javascript">


        function live_code_show() {

            var load_live_info = $.dialog.tips_black('正在加载直播信息，请稍后……', 15);
            $('#rtmp_url').val('');
            $('#rtmp_val').val('');
            $.ajax({
                type: "POST",
                url: "/specific/get_live_code",
                data: {room_id:335166},
                dataType: "json",
                success: function(ret) {
                    load_live_info.close();
                    $('#rtmp_url').val(ret.rtmp_send.rtmp_url);
                    $('#rtmp_val').val(ret.rtmp_send.rtmp_val);

                    $.dialog({
                        content: document.getElementById('live_code'),
                        id: 'live_tips',
                        title: '直播信息',
                        cancelVal: '关闭',
                        cancel: true,
                        lock: true,
                        padding: '10px 25px',
                        init: function() {
                            $('#copy_rtmp_url').zclip({
                                path: '/common/images/ZeroClipboard.swf',
                                copy: $('#rtmp_url').val(),
                                afterCopy: function() {
                                    $.dialog.tips_black("已成功复制rtmp地址", 1.5);
                                }
                            });
                            $('#copy_rtmp_val').zclip({
                                path: '/common/images/ZeroClipboard.swf',
                                copy: $('#rtmp_val').val(),
                                afterCopy: function() {
                                    $.dialog.tips_black("已成功复制直播码", 1.5);
                                }
                            });
                        }
                    });
                }
            });

        }



    </script>
</body>
</html>
