    function search_submit_left()
    {
        var word = $("#search_word").val();
        word = $.trim(word);
        if(word.length == 0)
        {
            alert_msg('搜索关键词还没有填写',"$('#search_word').focus();");
            return false;
        }
        window.location.href = '/search/' + encodeURIComponent(word) + '?from=left';
    }

